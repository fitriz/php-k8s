{
    "name": "fitriz/php-k8s",
    "description": "Control your Kubernetes clusters with this PHP-based Kubernetes client. It supports any form of authentication, the exec API, and it has an easy implementation for CRDs.",
    "keywords": [
        "laravel",
        "php",
        "kubernetes",
        "k8s",
        "k3s",
        "k0s",
        "cluster",
        "api",
        "kubeadm",
        "kubeapi",
        "kube"
    ],
    "license": "Apache-2.0",
    "homepage": "https://github.com/fitriz/php-k8s",
    "authors": [
        {
            "name": "Alex Renoki",
            "homepage": "https://github.com/rennokki",
            "role": "Developer"
        },
        {
            "name": "Fitri Zaini",
            "homepage": "https://github.com/fitriz",
            "role": "Developer"
        }
    ],
    "require": {
        "composer/semver": "^3.0@dev",
        "guzzlehttp/guzzle": "^7.0",
        "illuminate/macroable": "^11.0",
        "illuminate/support": "^11.0",
        "ratchet/pawl": "^0.4.1",
        "symfony/process": "^7.1.1"
    },
    "suggest": {
        "ext-yaml": "YAML extension is used to read or generate YAML from PHP K8s internal classes."
    },
    "autoload": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenokiCo\\PhpK8s\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^9.1.2",
        "phpstan/phpstan": "2.1.x-dev",
        "phpunit/phpunit": "^9.5.20|^10.0|^11.0",
        "vimeo/psalm": "^6.0"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
